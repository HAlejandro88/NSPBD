{
  "name": "sequelize-tuto",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "dev:unix": "DEBUG=sequelize-tuto:* nodemon src/app.js",
    "start": "node src/app.js",
    "lint": "eslint ./",
    "lint:fix": "eslint . --fix",
    "migrations:generate": "sequelize-cli migration:generate --name ",
    "migration:run": "sequelize-cli db:migrate",
    "migration:revert": "sequelize-cli db:migrate:undo",
    "migration:delete": "sequelize-cli db:migrate:undo:all",
    "debug-sequelize": "sequelize-cli db:migrate --debug",
    "format:check": "prettier --check .",
    "format:write": "prettier --write ."
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "debug": "^4.3.4",
    "express": "^4.18.1",
    "morgan": "^1.10.0",
    "mysql2": "^2.3.3",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.21.3",
    "sequelize-cli": "^6.4.1"
  },
  "devDependencies": {
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "prettier": "^2.7.1",
    "supertest": "^6.2.4",
    "supertest-fetch": "^1.5.0"
  }
}
